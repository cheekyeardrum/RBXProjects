-- ================= Auto-Collect Tab =================
local autoCollecting = false
local magnetRange = 5
local itemsFolder = workspace:WaitForChild("Game"):WaitForChild("MapItems")

local function buildAutoCollectTab()
    for _,v in pairs(contentFrame:GetChildren()) do v:Destroy() end

    local btn = Instance.new("TextButton", contentFrame)
    btn.Size = UDim2.new(0,250,0,50)
    btn.Position = UDim2.new(0,20,0,20)
    btn.Text = "Auto-Collect: OFF"
    btn.BackgroundColor3 = Color3.fromRGB(100,0,0)
    btn.TextColor3 = Color3.new(1,1,1)
    round(btn,6)

    local function updateStyle()
        if autoCollecting then
            btn.Text = "Auto-Collect: ON"
            btn.BackgroundColor3 = Color3.fromRGB(0,150,0)
        else
            btn.Text = "Auto-Collect: OFF"
            btn.BackgroundColor3 = Color3.fromRGB(100,0,0)
        end
    end

    btn.MouseButton1Click:Connect(function()
        autoCollecting = not autoCollecting
        updateStyle()

        if autoCollecting then
            task.spawn(function()
                local player = game.Players.LocalPlayer
                while autoCollecting do
                    local char = player.Character or player.CharacterAdded:Wait()
                    local hrp = char:WaitForChild("HumanoidRootPart")

                    -- ======= Magnet-style auto-collect for walk-over items =======
                    for _, item in ipairs(itemsFolder:GetChildren()) do
                        local basePart = item.PrimaryPart or item:FindFirstChildWhichIsA("BasePart")
                        if basePart then
                            pcall(function()
                                basePart.CFrame = hrp.CFrame * CFrame.new(0,0,-magnetRange)
                            end)
                        end
                    end

                    -- ======= Fire all Proximity Prompts (hold E items) =======
                    for _, prompt in pairs(workspace:GetDescendants()) do
                        if prompt:IsA("ProximityPrompt") then
                            -- Use Infinite Yield's internal firepp if available
                            if _G.iy and _G.iy.FireProximityPrompt then
                                _G.iy:FireProximityPrompt(prompt)
                            else
                                -- Fallback in case firepp isn't available
                              pcall(function()
                                  prompt:InputHoldBegin()
                                  task.wait(prompt.HoldDuration or 1)  -- wait for the required hold time
                                  prompt:InputHoldEnd()
                              end)
                            end
                        end
                    end

                    task.wait(0.1)
                end
            end)
        end
    end)

    updateStyle()
end

-- ================= Auto-Delete Tab =================
local function buildAutoDeleteTab()
    for _,v in pairs(contentFrame:GetChildren()) do v:Destroy() end
    local label = Instance.new("TextLabel", contentFrame)
    label.Size = UDim2.new(0.6,-10,0,30)
    label.Position = UDim2.new(0,10,0,10)
    label.BackgroundTransparency = 1
    label.Text = "Auto-Delete Pets"
    label.TextColor3 = Color3.new(1,1,1)
    label.Font = Enum.Font.SourceSansBold
    label.TextSize = 18
    label.TextXAlignment = Enum.TextXAlignment.Left

    local keepTxt = Instance.new("TextBox", contentFrame)
    keepTxt.Size = UDim2.new(0.35,0,0,30)
    keepTxt.Position = UDim2.new(0.65,0,0,10)
    keepTxt.PlaceholderText = "Keep Top N"
    keepTxt.Text = "50"
    keepTxt.ClearTextOnFocus = false
    round(keepTxt,6)

    local startBtn = Instance.new("TextButton", contentFrame)
    startBtn.Size = UDim2.new(0.35,0,0,30)
    startBtn.Position = UDim2.new(0.65,0,0,50)
    startBtn.Text = "Start"
    startBtn.Font = Enum.Font.SourceSansBold
    startBtn.TextSize = 16
    startBtn.BackgroundColor3 = Color3.fromRGB(88,101,242)
    startBtn.TextColor3 = Color3.new(1,1,1)
    round(startBtn,6)

    startBtn.MouseButton1Click:Connect(function()
        autoDelete = not autoDelete
        startBtn.Text = autoDelete and "STOP" or "Start"
        startBtn.BackgroundColor3 = autoDelete and Color3.fromRGB(200,50,50) or Color3.fromRGB(88,101,242)
        if autoDelete then
            task.spawn(function()
                local function collectSlots()
                    local t = {}
                    for _,c in ipairs(player.PlayerGui.Main.Center.Pets.Container.Pets:GetChildren()) do
                        if c:IsA("GuiObject") and c.Name:match("{[%w%-]+}") then
                            table.insert(t,c)
                        end
                    end
                    return t
                end
                local function getPetId(slot) return slot.Name end
                local keepTop = tonumber(keepTxt.Text) or 50
                while autoDelete do
                    local slots = collectSlots()
                    if #slots > keepTop then
                        table.sort(slots,function(a,b)
                            local ay = a.AbsolutePosition.Y
                            local by = b.AbsolutePosition.Y
                            if math.abs(ay - by) <= 4 then return a.AbsolutePosition.X < b.AbsolutePosition.X end
                            return ay < by
                        end)
                        for i = keepTop + 1, #slots do
                            local id = getPetId(slots[i])
                            if id then PetsRemote:FireServer({ Info="DeletePet", PetId=id }) end
                            task.wait(0.14)
                        end
                    end
                    task.wait(1)
                end
            end)
        end
    end)
end


-- ================= Auto-Equip Tab =================
local function buildAutoEquipTab()
    for _,v in pairs(contentFrame:GetChildren()) do v:Destroy() end

    local label = Instance.new("TextLabel", contentFrame)
    label.Size = UDim2.new(0.6,-10,0,30)
    label.Position = UDim2.new(0,10,0,10)
    label.BackgroundTransparency = 1
    label.Text = "Auto-Equip Top 6 Pets"
    label.TextColor3 = Color3.new(1,1,1)
    label.Font = Enum.Font.SourceSansBold
    label.TextSize = 18
    label.TextXAlignment = Enum.TextXAlignment.Left

    local startBtn = Instance.new("TextButton", contentFrame)
    startBtn.Size = UDim2.new(0.35,0,0,30)
    startBtn.Position = UDim2.new(0.65,0,0,10)
    startBtn.Text = "Start"
    startBtn.Font = Enum.Font.SourceSansBold
    startBtn.TextSize = 16
    startBtn.BackgroundColor3 = Color3.fromRGB(88,101,242)
    startBtn.TextColor3 = Color3.new(1,1,1)
    round(startBtn,6)

    local equipping = false
    startBtn.MouseButton1Click:Connect(function()
        equipping = not equipping
        startBtn.Text = equipping and "STOP" or "Start"
        startBtn.BackgroundColor3 = equipping and Color3.fromRGB(200,50,50) or Color3.fromRGB(88,101,242)

        if equipping then
            task.spawn(function()
                local function collectSlots()
                    local t = {}
                    for _,c in ipairs(player.PlayerGui.Main.Center.Pets.Container.Pets:GetChildren()) do
                        if c:IsA("GuiObject") and c.Name:match("{[%w%-]+}") then
                            table.insert(t,c)
                        end
                    end
                    return t
                end

                local function getPetId(slot)
                    return slot.Name
                end

                while equipping do
                    local slots = collectSlots()
                    if #slots > 0 then
                        table.sort(slots,function(a,b)
                            local ay = a.AbsolutePosition.Y
                            local by = b.AbsolutePosition.Y
                            if math.abs(ay - by) <= 4 then
                                return a.AbsolutePosition.X < b.AbsolutePosition.X
                            end
                            return ay < by
                        end)

                        -- Equip the top 6 pets
                        for i = 1, math.min(6, #slots) do
                            local id = getPetId(slots[i])
                            if id then
                                PetsRemote:FireServer({ Info="EquipPet", PetId=id })
                                task.wait(0.2)
                            end
                        end
                    end
                    task.wait(3) -- re-check every few seconds
                end
            end)
        end
    end)
end



-- Tab Buttons
local tabFuncs = {buildMineTab,buildWheelTab,buildRewardsTab,buildTeleportTab,buildEggsTab,buildAutoWinTab,buildAutoCollectTab,buildAutoDeleteTab,buildAutoEquipTab}
for i,name in ipairs(tabNames) do
    local btn = Instance.new("TextButton", tabsFrame)
    btn.Size = UDim2.new(1,0,0,40)
    btn.Position = UDim2.new(0,0,0,(i-1)*45)
    btn.Text = name
    btn.Font = Enum.Font.SourceSansBold
    btn.TextSize = 16
    btn.BackgroundColor3 = Color3.fromRGB(50,50,50)
    btn.TextColor3 = Color3.new(1,1,1)
    round(btn,6)
    local func = tabFuncs[i]
    btn.MouseButton1Click:Connect(func)
    table.insert(tabButtons, btn)
end

-- Close/Minimize
closeBtn.MouseButton1Click:Connect(function() screenGui:Destroy() end)
minBtn.MouseButton1Click:Connect(function()
    if frame.Size==HUB_SZ then
        tween(frame,{Size=MINIMIZED_SZ})
    else
        tween(frame,{Size=HUB_SZ})
    end
end)

-- Default tab
buildMineTab()
